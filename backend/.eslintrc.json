{
  "parserOptions": {
    "ecmaVersion": 10
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-labels": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, {"functions": false, "classes": false}],
    "callback-return": 2,
    "no-sync": 1,
    "no-invalid-regexp": 2,
    "no-undef": 2
  }
}
